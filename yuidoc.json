{
  "name": "Ember Validators",
  "description": "A collection of EmberJS validators",
  "url": "https://github.com/offirgolan/ember-validators",
  "indexModule": "Validators",
  "options": {
    "enabledEnvironments": ["development", "production"],
    "paths": [
      "app",
      "addon"
    ],
    "external": {
      "data": [{
        "base": "http://emberjs.com/api/",
        "json": "http://builds.emberjs.com/tags/v2.10.0/ember-docs.json"
      }]
    },
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": ["node_modules/yuidoc-ember-theme/helpers/helpers.js"],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "nocode": false
  }
}
